{
  "name": "generator-<%= name %>",
  "version": "<%= version %>",
  "description": "<%= description %>",
  "keywords": ["yeoman-generator", "generator", "yo", "yeoman"],
  "main": "generators/app/index.js",
  "files": ["generators"],
  "scripts": {
    "build": "npm run clean && babel src -d generators/app",
    "clean": "rm -rf demo generators/*/*.js",
    "link": "npm link && npm link generator-<%= name %>",
    "lint": "eslint ./",
    "lint:fix": "eslint --fix ./",
    "prepublish": "npm run build",
    "start":
      "npm run clean && npm run lint && npm run build && yo --destination=demo <%= name %>",
    "unlink": "npm unlink generator-<%= name %>"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "empty-dir": "^0.2.1",
    "fs-extra-promise": "^1.0.1",
    "git-user-email": "git+https://git@github.com/jamrizzi/git-user-email.git",
    "git-user-name": "^2.0.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "yeoman-generator": "^1.0.0",
    "yeoman-option-or-prompt":
      "git+https://github.com/artefact-group/yeoman-option-or-prompt.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.18.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "yo": "^2.0.0"
  },
  "author": "<%= authorName %> <<%= authorEmail %>>",
  "homepage": "<%= homepage %>",
  "license": "<%= license %>",
  "repository": "<%= repository %>",
  "eslintIgnore": ["generators", "demo"],
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "4"
          }
        }
      ],
      "stage-2"
    ]
  }
}
