{
  "name": "generator-yo-es6",
  "version": "1.0.8",
  "description": "ðŸŽ© A generator for ES6 yeoman generators",
  "keywords": [
    "yeoman-generator",
    "generator",
    "yo",
    "yeoman",
    "es6"
  ],
  "main": "generators/app/index.js",
  "files": [
    "generators"
  ],
  "scripts": {
    "build": "npm run test && babel src -d generators/app",
    "clean": "rimraf demo generators/*/*.js",
    "format": "prettier --write ./src/**/*.js",
    "link": "npm link && npm link generator-yo-es6",
    "lint": "npm run format && eslint ./",
    "lint:fix": "npm run format && eslint --fix ./",
    "prepublish": "npm run clean && npm run build",
    "start": "npm run clean && npm run test && npm run build && yo --destination=demo yo-es6",
    "test": "npm run lint",
    "unlink": "npm unlink generator-yo-es6"
  },
  "dependencies": {
    "generator-github-project": "^0.2.8",
    "idempotent-babel-polyfill": "^7.0.0",
    "lodash": "^4.17.11",
    "project-guess": "^0.2.4",
    "yeoman-generator": "^1.0.0",
    "yo-base-prompts": "^0.1.6",
    "yo-option-or-prompt": "^0.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-preset-everything": "^0.1.2",
    "eslint": "^5.6.1",
    "eslint-config-jam": "^0.1.4",
    "rimraf": "^2.6.2",
    "yo": "^2.0.5"
  },
  "author": "Jam Risser <jam@jamrizzi.com>",
  "homepage": "https://github.com/jamrizzi/generator-yo-es6",
  "license": "MIT",
  "repository": "https://github.com/jamrizzi/generator-yo-es6",
  "eslintIgnore": [
    "generators",
    "demo"
  ]
}
