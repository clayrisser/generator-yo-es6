{
  "name": "generator-yohoho",
  "version": "v0.2.0",
  "description": "A pragmatic implementation of TrailsJS for rapid development",
  "keywords": [
    "yeoman-generator",
    "generator",
    "yo",
    "yeoman"
  ],
  "main": "app/index.js",
  "files": [
    "app"
  ],
  "scripts": {
    "link": "npm link && npm link generator-yohoho",
    "start": "npm run clean && npm run lint && npm run build && node ./node_modules/yo/lib/cli --destination=demo yohoho",
    "build": "rm -rf ./app/*.js && node ./node_modules/jnwb/lib/bin/nwb build && mkdir -p ./app && cp -r ./lib/* ./app",
    "prepublish": "npm run build",
    "test": "node ./node_modules/jnwb/lib/bin/nwb test",
    "test:coverage": "node ./node_modules/jnwb/lib/bin/nwb test --coverage",
    "test:watch": "node ./node_modules/jnwb/lib/bin/nwb test --server",
    "lint": "node ./node_modules/eslint/bin/eslint ./",
    "lint:fix": "node ./node_modules/eslint/bin/eslint --fix ./",
    "clean": "rm -rf ./app/*.js ./es ./lib ./demo"
  },
  "dependencies": {
    "empty-dir": "^0.2.1",
    "fs-extra-promise": "^1.0.1",
    "git-user-email": "git+https://git@github.com/jamrizzi/git-user-email.git",
    "git-user-name": "^2.0.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "yeoman-generator": "^1.0.0",
    "yeoman-option-or-prompt": "git+https://github.com/artefact-group/yeoman-option-or-prompt.git"
  },
  "devDependencies": {
    "eslint": "^4.14.0",
    "expect": "^22.0.3",
    "jnwb": "^0.1.5",
    "yo": "^2.0.0"
  },
  "author": "Jam Risser <jam@jamrizzi.com>",
  "homepage": "https://github.com/jamrizzi/generator-yohoho",
  "license": "MIT",
  "repository": "https://github.com/jamrizzi/generator-yohoho"
}
